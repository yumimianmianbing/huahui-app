{
  "version": 2,
  "builds": [
    {
      "src": "backend/api/index.py",
      "use": "@vercel/python@3.1.7",
      "config": {
        "maxLambdaSize": "15mb",
        "buildCommand": "pip install -r requirements.txt"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/backend/api/index.py",
      "methods": ["GET", "POST", "OPTIONS"]
    }
  ]
}