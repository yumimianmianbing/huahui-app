{
  "version": 2,
  "builds": [
    {
      "src": "backend/api/index.py",
      "use": "@vercel/python@2.0.1",
      "config": {
        "maxLambdaSize": "15mb",
        "pythonVersion": "3.9" , 
        "installCommand": "pip3.9 install -r requirements.txt" 
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/backend/api/index.py",
      "methods": ["GET", "POST", "OPTIONS"]
    }
  ]
}