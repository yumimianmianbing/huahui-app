{
  "version": 2,
  "builds": [
    {
      "src": "backend/api/index.py",
      "use": "@vercel/python@3.0.2",
      "config": {
        "maxLambdaSize": "15mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/backend/api/index.py",
      "methods": ["GET", "POST", "OPTIONS"]
    }
  ]
}