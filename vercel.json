{
  "version": 2,
  "builds": [
    {
      "src": "backend/api/index.py",
      "use": "@vercel/python@2.0.1",
      "config": {
        "maxLambdaSize": "15mb",
        "pythonVersion": "3.9",
        "installCommand": "pip3 install --disable-pip-version-check --target . --upgrade -r /vercel/path0/requirements.txt -i https://mirrors.aliyun.com/pypi/simple/"
       
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/backend/api/index.py",
      "methods": ["GET", "POST", "OPTIONS"]
    }
  ]
}